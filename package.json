{
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "type-check": "tsc",
    "test": "jest --coverage",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-vue": "^2.2.5",
    "@types/mocha": "^8.0.4",
    "@vue/test-utils": "^2.0.0-beta.10",
    "jest": "^24.0.0",
    "snowpack": "^2.17.1",
    "typescript": "^4.1.2",
    "vue-jest": "^5.0.0-alpha.6"
  },
  "name": "holonet"
}
